<%- include("partials/youtube/youtubeResults") -%>
<%- include("partials/navbar") -%>

<div class="container mt-3">
  <h3>Results for "<%- query -%>"</h3>

  <% videos.forEach(video => { %>
    <div class="my-5">
      <h4><a href="https://www.layluh.com/watch?v=<%- video.id -%>"><%- video.title -%></a></h4>  
      <% let date = video.publishedAt.toLocaleDateString() %>
      <p><%- video.channelTitle -%> - <%- date -%></p>
    </div>

  <% }) %>
</div>


<%- include("partials/genericFooter") -%>